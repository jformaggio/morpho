{
    "stan": {
        
        "name": "Q_generator",
        
        "model":   {
            "file": "./models/Q_generator.stan",
            "function_file": "./models/Q_functions.stan",
            "cache": "./cache"
        },
        
        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./data/Q_generate.data",
                      "format":"R"
                      }
                      ],
            "parameters": [
                           {
                           "chainnum" : 4,
                           "iternum" : 2000,
                           }
                           ]
            
        },
        
        "run": {
            "algorithm": "NUTS",
            "iter": 2000,
            "chain": 4},
        
        "sample": "./results/Qtest_generator.out",
        
        "output":
        {
            "name" : "./results/Qtest_generator",
            "format": "hdf5",
            "fit": "./results/Qresults1.pkl",
            "config": {"group": "/"},
            "data": [
                     {"stan_parameter": "Q","output_name": "Q"},
                     {"stan_parameter": "sigma","output_name": "sigma"},
                     ]
        }
        
    }
    
    
}