{
  "stan": {

      "name": "krypton_RSA_analysis",

    "model":   {
       "file": "./krypton_run/models/krypton_RSA_analysis.stan",
      "cache": "./krypton_run/cache"
      },

    "data": {
       "type": "data",
      "files": [
          {
         "name": "./krypton_run/data/krypton_frequency_run.header.data", 
         "format":"R"
          },
           {
         "name" : "./krypton_run/data/RSAdatav3_20140903_30keV_tthreshcut.data", 
         "format" : "R"
          }],
      "parameters": [
            {
         "BField" : 0.953382,
         "TrapCurrent" : 400,
         "minFreq" : 25100000000.0,
         "maxFreq" : 25200000000.0,
         "nSignals" : 3
            }
          ]
   },

   "run": {
          "algorithim": "NUTS",
          "iter": 10000,
          "warmup": 5000,
          "chain": 1,
          "init": [
            {"uNormal": [0.0,0.0],
             "uCauchy": 0.0,
             "GlobalWidth" : 15.0,
             "SourceMean":[30440.0, 30477.0, 30250.0],
             "SourceWidth" :[2.0, 2.0, 2.0],
             "SourceStrength" :[0.6, 0.3, 0.1]
            }
             ]
          },
   "plot": [
      {"variable": "MainField"},
      {"variable": "TrappingField"},
      {"variable": "TotalField"},
      {"variable": "SourceMean"},
      {"variable": "SourceWidth"},      
      {"variable": "SourceSkew"},
      {"variable": "lp__"}            
      ],

   "output": 
        {
         "name" : "./krypton_run/results/RSA_30keV_analysis.root",
         "format" : "root",
         "tree" : "stanAnalysis",
         "branches" : [
               {"variable" : "SourceStrength", "root_alias" : "SourceStrength", "ndim":3},
               {"variable" : "SourceMean", "root_alias" : "SourceMean","ndim":3},
               {"variable" : "SourceWidth", "root_alias" : "SourceWidth","ndim":3},
               {"variable" : "GlobalWidth", "root_alias" : "SourceSkew"},
               {"variable" : "freq_gen", "root_alias" : "freq_gen"},
               {"variable" : "energy_gen", "root_alias" : "energy_gen"},
               {"variable" : "MainField", "root_alias" : "MainField"},
               {"variable" : "TrappingField", "root_alias" : "TrappingField"},
               {"variable" : "TotalField", "root_alias" : "TotalField"},
               {"variable" : "lp__", "root_alias" : "LogLikelihood"}
                ]
         }      
   }

}
